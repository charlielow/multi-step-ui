{
  "extends": "airbnb",
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  rules: {
    "no-underscore-dangle": 0,
    "no-param-reassign": 1,
    "prefer-template": 0,
    "comma-dangle": 1,
    "no-unused-vars": 1,
    "arrow-body-style": 0,
    "class-methods-use-this": 0,
    "object-shorthand": 1,
    "max-len": 0
  }
}